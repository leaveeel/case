    var iframe = document.createElement("iframe");
    var ifr =   document.getElementById("ifr"); 
function throwIframe()
{
    iframe.src="http://www.baidu.com/";
    iframe.width="580";
    iframe.height="510";
    iframe.style.border="0";
    iframe.scrolling="no";//iframe内的滚动条
    ifr.style.display="block";
    ifr.style.border="1px solid #ccc";
    ifr.appendChild(iframe);
    ifr.style.width="580px";
    ifr.style.height="510px";
    document.getElementById("ifrmax").style.display="none";
    document.getElementById("ifrmin").style.display="block";
}

function min(){
    ifr.style.width="250px";
    ifr.style.height="60px";
    document.getElementById("ifrmin").style.display="none";
    document.getElementById("ifrmax").style.display="block";
}
function max(){
    ifr.style.width="580px";
    ifr.style.height="510px";
    document.getElementById("ifrmax").style.display="none";
    document.getElementById("ifrmin").style.display="block";
}
function clo(){
    ifr.removeChild(iframe);
    ifr.style.display="none";
}
